@font-face {
    font-family: 'm-fe-all';
    src:    url('./fonts/m-fe-all.eot?eczikk');
    src:    url('./fonts/m-fe-all.eot?eczikk#iefix') format('embedded-opentype'),
        url('./fonts/m-fe-all.ttf?eczikk') format('truetype'),
        url('./fonts/m-fe-all.woff?eczikk') format('woff'),
        url('./fonts/m-fe-all.svg?eczikk#m-fe-all') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'm-fe-all' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

